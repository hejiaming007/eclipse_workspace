<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:jdbc="http://www.springframework.org/schema/jdbc"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
		http://www.springframework.org/schema/jdbc http://www.springframework.org/schema/jdbc/spring-jdbc-3.0.xsd"
		>
		
	
	<!-- refer to other application context file, relative path -->
	<!-- tiral IoC module -->
	<import resource="spring/ioc/iocApplicationContext.xml"/>
	
	<!-- tiral i18n module -->
	<import resource="spring/i18n/i18nApplicationContext.xml"/>
	
	<!-- tiral jdbc module -->
	<import resource="spring/jdbc/jdbcApplicationContext.xml"/>
		
	<!-- trial jmx module -->	
	<import resource="spring/jmx/jmxApplicationContext.xml"/>
		
	
</beans>